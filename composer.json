{
    "name": "nguyentrigiang/laravel-notification",
    "description": "A Package for management notification with jsonapi specification",
    "keywords": [
        "nguyentrigiang",
        "notification"
    ],
    "homepage": "https://github.com/giangnguyentri/notifications",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "GiangNT",
            "email": "nguyentrigiang1991@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "berkayk/onesignal-laravel": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
      "psr-4": {
        "GiangNT\\LaravelNotification\\": "src/"
      }
    },
    "autoload-dev": {
        "psr-4": {
            "GiangNT\\LaravelNotification\\": "src/",
            "GiangNT\\LaravelNotification\\Tests\\": "tests/"
        }
    },
    "extra": {
      "laravel": {
          "providers": [
              "GiangNT\\LaravelNotification\\NotificationServiceProvider"
          ]
      }
    }
}
